		zb=$pjq('<div></div>').addClass('zonesignboard');
		name=$pjq('<p/>').addClass('zonename').html(value.col2[0]);
		name.appendTo(zb);
	}
	
	if (ft=='N/A'){
	}else {
		var fdl=$pjq('<div></div>').addClass('dangerlevel').addClass(fc).html(ft);
		fdl.appendTo(zb);
	}
	if (ifpl=='N/A'){
		//do nothing on IFPL
	}else {
		var ifplt='IFPL '+ifpl;
		var ifpldiv=$pjq('<div></div>').addClass('dangerlevel').addClass('IFPL').html(ifplt);
		ifpldiv.appendTo(zb);
	}
	var divfl2=$pjq('#fl2');	
	if ((ft=='N/A') && (ifpl=='N/A')){
	}else {
		zb.appendTo(divfl2);
	}
}

function renderURLSignBoard(ur){
	if (ur.length!=0) {
		var zoneRopeImg=$pjq('<img/>').attr('src',rope).attr('alt','Chains').addClass('zoneimg');
		var divfl2=$pjq('#fl2');	
		zoneRopeImg.appendTo(divfl2);
		var usb=$pjq('<div/>').attr('id','URLsignboard').attr('class','box').css('text-align','left');
		var ul=$pjq('<ul/>');
		ul.appendTo(usb);
		$pjq.each(ur,function(key , value){
			var li=$pjq('<li/>');
			li.appendTo(ul);
			var a=$pjq('<a/>').attr('href',value.col3[0]).attr('target','_blank').html(value.col2[0]);
			a.appendTo(li);
		});
		var divfl2=$pjq('#fl2');	
		usb.appendTo(divfl2);
	}
}
function renderZones(fdlz,ur){
	//alert(JSON.stringify(fdlz));
	
	$pjq.each(fdlz,function(key , value){
		//alert(key);
		//alert(JSON.stringify(value));
		renderZoneBoard(key,value);
		var zoneRopeImg=$pjq('<img/>').attr('src',rope).attr('alt','Chains').addClass('zoneimg');
		var divfl2=$pjq('#fl2');	
		if (key==(fdlz.length-1)) {
		}else {
			zoneRopeImg.appendTo(divfl2);
		}
	});		
	renderURLSignBoard(ur);
	//alert(JSON.stringify(ur));
}

function forestWide(f,ifpl){
	//alert(f);
	//alert(ifpl);

	var ft=fireLevelText(f);
	//alert(ft);
	var fc=fireLevelClass(f);
	var zb=$pjq('<div></div>').addClass('zonesignboard');
	//alert(f);
	if (f=='N/A'){
	}else {
		var fdl=$pjq('<div></div>').addClass('dangerlevel').addClass(fc).html(ft);
		fdl.appendTo(zb);
	}
	if (ifpl=='N/A'){
		//do nothing on IFPL
	}else {
		var ifplt='IFPL '+ifpl;
		var ifpldiv=$pjq('<div></div>').addClass('dangerlevel').addClass('IFPL').html(ifplt);
		ifpldiv.appendTo(zb);
	}
	var divfl2=$pjq('#fl2');	
	zb.appendTo(divfl2);
	//alert(fc);	
};
function fireLevelClass(l){
	var ret;
	switch (l){
		case '1':
			ret='dllow';	
			break;
		case '2':
			ret='dlmoderate';
			break;
		case '3':
			ret='dlhigh';
			break;
		case '4':
			ret='dlveryhigh';
			break;
		case '5':
			ret='dlextreme';
			break;
		default :
			ret='dllow';
			break;
	}
	return ret;
	
};

function ifplRoman(ifpl){
	var ret;
	switch(ifpl){
		case '1':
			ret='I';	
			break;
		case '2':
			ret='II';
			break;
		case '3':
			ret='III';
			break;
		case '4':
			ret='IV';
			break;
		case 'na':
			ret='N/A';
			break;
		default :
			ret=99;
			break;
	}
	return ret;
};

function fireLevelNum(t){
	var ret;
	switch(t){
		case 'Low':
			ret='1';	
			break;
		case 'Moderate':
			ret='2';
			break;
		case 'High':
			ret='3';
			break;
		case 'Very High':
			ret='4';
			break;
		case 'Extreme':
			ret='5';
			break;
		case 'N/A':
			ret='N/A';
			break;
		default :
			ret='99';
			break;
	}
	return ret;
};

function fireLevelTextFromNumeric(l){
	var ret;
	switch (l){
		case 1:
			ret='LOW';	
			break;
		case 2:
			ret='MODERATE';
			break;
		case 3:
			ret='HIGH';
			break;
		case 4:
			ret='VERY HIGH';
			break;
		case 5:
			ret='EXTREME';
			break;
		//case  '-1':
		case  'na':
			ret='N/A';
			break;
		default :
			ret='N/A';
			break;
		
	}
	return ret;
	
};


function fireLevelText(l){
	var ret;
	switch (l){
		case '1':
			ret='LOW';	
			break;
		case '2':
			ret='MODERATE';
			break;
		case '3':
			ret='HIGH';
			break;
		case '4':
			ret='VERY HIGH';
			break;
		case '5':
			ret='EXTREME';
			break;
		//case  '-1':
		case  'na':
			ret='N/A';
			break;
		default :
			ret='N/A';
			break;
		
	}
	return ret;
	
};

$pjq(document).ready(function(){
	if (fireLevel2==null){
		//alert(JSON.stringify(fireLevel1));
		//carry over fireLevel1 value
		smokeyFireSign();
		forestWide(fireLevel1.toString(),'N/A');
	}else {
		//alert(JSON.stringify(fireLevel2));
		if (fireLevel2.dangerLevelAt=='forest'){
			var fln=fireLevelNum(fireLevel2.fwLevel);
			if ((fln=='N/A') && (fireLevel2.ifplLevel=='N/A') ){
				//if both are N/A don't display anything
			}else {
				smokeyFireSign();
				forestWide(fln,fireLevel2.ifplLevel);
				renderURLSignBoard(fireLevel2.ur);
			}


		}else{
			smokeyFireSign();
			renderZones(fireLevel2.fdlz,fireLevel2.ur);
			
		}
	}
});
</script>

